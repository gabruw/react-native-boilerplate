{
    "clearMocks": true,
    "collectCoverage": true,
    "preset": "react-native",
    "cacheDirectory": ".jest/cache",
    "coverageDirectory": ".jest/coverage",
    "moduleFileExtensions": ["js", "ts", "tsx"],
    "moduleDirectories": ["node_modules", "src"],
    "testRegex": "(/__tests__/.*|\\.test)\\.(js|ts|tsx)$",
    "setupFiles": ["<rootDir>/src/__mocks__/setup/index.ts"],
    "setupFilesAfterEnv": ["@testing-library/jest-native/extend-expect"],
    "globals": {
        "ts-jest": {
            "babelConfig": true
        }
    },
    "transform": {
        "^.+\\.(js|ts|tsx)$": [
            "babel-jest",
            {
                "rootMode": "upward"
            }
        ]
    },
    "testPathIgnorePatterns": [
        "<rootDir>/.jest/",
        "<rootDir>/.expo/",
        "<rootDir>/.husky/",
        "<rootDir>/.github/",
        "<rootDir>/.vscode/",
        "<rootDir>/index.js",
        "<rootDir>/android/",
        "<rootDir>/src/assets/",
        "<rootDir>/src/App.tsx",
        "<rootDir>/src/Main.tsx",
        "<rootDir>/.stryker-tmp/",
        "<rootDir>/node_modules/",
        "<rootDir>/src/constants/",
        "<rootDir>/src/animations/",
        "<rootDir>/src/extensions/",
        "<rootDir>/src/*/__mocks__/",
        "<rootDir>/src/*/__fixtures__/"
    ],
    "modulePathIgnorePatterns": [
        "<rootDir>/.jest/",
        "<rootDir>/.expo/",
        "<rootDir>/.husky/",
        "<rootDir>/.github/",
        "<rootDir>/.vscode/",
        "<rootDir>/index.js",
        "<rootDir>/android/",
        "<rootDir>/src/assets/",
        "<rootDir>/src/App.tsx",
        "<rootDir>/src/Main.tsx",
        "<rootDir>/.stryker-tmp/",
        "<rootDir>/node_modules/",
        "<rootDir>/src/constants/",
        "<rootDir>/src/animations/",
        "<rootDir>/src/extensions/",
        "<rootDir>/src/*/__mocks__/",
        "<rootDir>/src/*/__fixtures__/"
    ],
    "coveragePathIgnorePatterns": [
        "<rootDir>/.jest/",
        "<rootDir>/.expo/",
        "<rootDir>/.husky/",
        "<rootDir>/.github/",
        "<rootDir>/.vscode/",
        "<rootDir>/index.js",
        "<rootDir>/android/",
        "<rootDir>/src/assets/",
        "<rootDir>/src/App.tsx",
        "<rootDir>/src/Main.tsx",
        "<rootDir>/.stryker-tmp/",
        "<rootDir>/node_modules/",
        "<rootDir>/src/constants/",
        "<rootDir>/src/animations/",
        "<rootDir>/src/extensions/",
        "<rootDir>/src/*/__mocks__/",
        "<rootDir>/src/*/__fixtures__/"
    ]
}
